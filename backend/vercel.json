{
    "version": 2,
    "builds": [
        {"src": "backend/src/**/*.ts", "use": "@vercel/node"}
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/src/app.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/backend/src/server.ts"
        }
    ]
}



