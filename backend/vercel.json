{
    "version": 2,
    "builds": [
        {"src": "backend/src/*.ts", "use": "@vercel/node"}
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/src/routes/api.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/backend/src/routes/api.ts"
        }
    ]
}

